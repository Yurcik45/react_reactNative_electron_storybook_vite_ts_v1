{
  "compilerOptions": {
    "target": "esnext",
    "lib": [
      "es2017",
      "ES6",
      "dom"
    ],
    "jsx": "react-native",
    "module": "commonjs",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "allowJs": true,
    "noEmit": true,
    "isolatedModules": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true
  },
  "extends": "expo/tsconfig.base"
}
